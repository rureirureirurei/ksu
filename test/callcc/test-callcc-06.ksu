;100
;Local set variables save their state during continuation switches
(define saved-k (box #f))
(define sum (box 0))

(let ([x (box 10)]) (begin
  (call/cc k (set! saved-k k))
  (set! sum (+ (unwrap sum) (unwrap x)))
  (set! x 30)
  (set! sum (+ (unwrap sum) (unwrap x)))
))

((unwrap saved-k) (- 0 1))
(print (peek sum))
